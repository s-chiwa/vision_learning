        import xml.etree.ElementTree as ET
        path = self.memory.getData("pathPostVisonLearning")
        fileManual = os.path.join(path ,"vision_learn_manual.xml")
        if not os.path.isfile(fileManual):
            #作業を指示するファイルがない場合は終了します。
            self.onMissingManual()
        else:
            tree = ET.parse(fileManual)
            root = tree.getroot()
            knowledge = ALProxy("ALKnowledge")
            #visionReco = ALProxy("ALVisionRecognition")
            #knowledge.add("paramSystem","あああ","visionReco","abc")

            stateMess =  ""

            for databaseNode in root.iter("database"):
                dbName = databaseNode.find("databaseName").text
                #self.logger.info(dbName)
                dbLabel = databaseNode.find("databaseLabel").text.encode("utf-8")
                #self.logger.info(dbLabel)
                label = knowledge.getObject("paramSystem",dbLabel,"visionReco")
                #self.logger.info(label)
                if len(label) == 1:
                    if label[0] != dbName:
                        stateMess = stateMess + "「" + dbLabel + "」という言葉で呼び出せる画像認識データベースは「" + dbName + "」という名前ですでにあります。" + ","
