    def onInput_onStart(self):
        import shutil
        fileRec = self.memory.getData("executedFileVisonLearning")
        fileNot = self.memory.getData("notRecPictureVisonLearning")
        self.logger.info("files rec:"+str(fileRec))
        self.logger.info("files not:"+str(fileNot))

        pathPost = self.memory.getData("pathServeVisonLearning")
        pathExe = self.memory.getData("pathExecutedVisonLearning")
        self.logger.info("path post:"+pathPost)
        self.logger.info("path exe:"+pathExe)

        import datetime
        today = datetime.datetime.today()
        dateRecord = today.strftime("%Y-%m-%d_%H-%M-%S")
        pathExecuted = os.path.join(pathExe,dateRecord)
        if not os.path.isdir(pathExecuted):
            os.makedirs(pathExecuted)

        for fileName in fileRec:
            self.logger.info(os.path.join(pathPost,fileName)+":::"+ os.path.join(pathExecuted,dateRecord,fileName))
            shutil.copy(os.path.join(pathPost,fileName), os.path.join(pathExecuted,dateRecord+fileName))

        exeLog = self.memory.getData("recentResultVisonLearning")


        self.onStopped(today.strftime("%Y年%m月%d日%H時%M分%S秒")+"というフォルダを作って、学習したファイルを移動しました。"+exeLog)

    def onInput_onStop(self):
