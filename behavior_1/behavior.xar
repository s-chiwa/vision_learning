            for file_ in files:
                full_path = os.path.join(root, file_)

                if full_path.endswith(".top"):
                    self.logger.info(full_path)
                    f = open(full_path)
                    lines = f.readlines()
                    f.close()

                    for ln in lines:
                        if ln.startswith("u:(") and ln.find("e:") == -1:
                            start = len("u:(")
                            end = ln.find(")")
                            #self.logger.info(ln[start:end])
