                            <script language="4">
                                <content>
                                    <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        pass

    def onLoad(self):
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        self.memory = None

    def onInput_onStart(self):
        #moveToの初期設定移動量
        self.memory.insertData("walkDistMoveTo",.2)
        self.memory.insertData("walkLittleDistMoveTo",0.1)

        #motionの初期設定動作量
        self.memory.insertData("degreeMoveAction",8.0)

        #作業用フォルダの確保
        pathRoot = ""

        if os.path.isdir("/home/nao/"):
            pathRoot = "/home/nao"
        else:
            #localhostでは適当な作業ディレクトリを指定してください
            pathRoot = "c:\\temp"

        pathPost = os.path.join(pathRoot,"workfile_store","post","vision_learning")
        pathServe = os.path.join(pathRoot,"workfile_store","serve","vision_learning")
        pathExecuted = os.path.join(pathRoot,"workfile_store","executed","vision_learning")

        self.memory.insertData("pathPostVisonLearning",pathPost)
        self.memory.insertData("pathServeVisonLearning",pathServe)
        self.memory.insertData("pathExecutedVisonLearning",pathExecuted)

        if not os.path.isdir(pathPost):
            os.makedirs(pathPost)

        if not os.path.isdir(pathServe):
            os.makedirs(pathServe)

        if not os.path.isdir(pathExecuted):
            os.makedirs(pathExecuted)

        #汎用メッセージ保存用
        recentResult = ""
        self.memory.insertData("recentResultVisonLearning",recentResult)
        #処理済みファイル名と未処理ファイルを常に保存
        fileGroup = []
        self.memory.insertData("notRecPictureVisonLearning",fileGroup)
        self.memory.insertData("executedFileVisonLearning",fileGroup)

        #画像認識データベースのうちcurrent(初期設定)とnowTemp(作業用)を予約
        knowledgeVision = ALProxy("ALKnowledge")

        knowledgeVision.add("visionLearningArea", "システム初期設定" , "databaseName", "current")
        knowledgeVision.add("visionLearningArea", "システム初期設定" , "headPitch", "0")

        knowledgeVision.add("visionLearningArea", "撮影作業領域" , "databaseName", "nowTemp")
        knowledgeVision.add("visionLearningArea", "撮影作業領域" , "headPitch", "0")

        dist = knowledgeVision.getObject("visionLearningArea", "move" , "distance")
        theta = knowledgeVision.getObject("visionLearningArea", "head" , "theta")

        if len(dist) == 0:
            knowledgeVision.add("visionLearningArea", "move" , "distance", "0.1")
        import math
        if len(theta) == 0:
            knowledgeVision.add("visionLearningArea", "head" , "theta", str(math.radians(4)))

        self.memory.insertData("tempHeadPitchVisionLearning",-9999.0)
        #self.memory.insertData("nowDatabaseVisionLearning",)


        #Pepperちゃんの画像認識データベースを呼ぶ言葉
        self.memory.insertData("wordsCallVisionDBVisionLearning","部屋")
        #Pepperちゃんの画像認識できたら話す言葉
        self.memory.insertData("wordsRecognitionVisionLearning","この部屋")
        self.memory.insertData("nameRecoDatabaseVisionDBVisionLearning","999")

        self.memory.insertData("resolutionVisionLearning",2)#640*480
        self.memory.insertData("cameraIdVisionLearning",0)#top

        self.memory.insertData("moveDistanceVisionLearning",0.1)#10cm
        self.memory.insertData("turnThetaVisionLearning",7)#7度
        self.memory.insertData("timesActionThetaVisionLearning",1)#繰り返しの回数:使っていません。

        self.memory.insertData("poseRadianVisionLearning",0)#頭の高さ=視線





        self.onStopped("")

    def onInput_onStop(self):
        self.onUnload()]]>
