                            if baseWords.find("~") != -1:
                                st = baseWords.find("~")
                                en = baseWords.find(" ")
                                #self.logger.info("****"+baseWords[st:en])
                                baseWords = baseWords.replace(baseWords[st:en],"【ユーザ選択肢】")
