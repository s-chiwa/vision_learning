        pass

    def onLoad(self):
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        self.memory = None

    def onInput_onStart(self):
        self.memory.insertData("recentResultVisonLearning","")
        import xml.etree.ElementTree as ET
        path = self.memory.getData("pathPostVisonLearning")
        fileManual = os.path.join(path ,"vision_learn_manual.xml")
        tree = ET.parse(fileManual)
        root = tree.getroot()
        knowledge = ALProxy("ALKnowledge")
        visionReco = ALProxy("ALVisionRecognition")
        dbPath = visionReco.getParam("db_path")
        #dbNamePre = visionReco.getParam("db_name")
        exeLog = ""

        knowledgeVision = ALProxy("ALKnowledge")

        fileGroupRec = []
        fileGroupNot = []

        for databaseNode in root.iter("database"):
            dbName = databaseNode.find("databaseName").text
            dbLabel = databaseNode.find("databaseLabel").text.encode("utf-8")
            headPitch = databaseNode.find("headPitch").text
            knowledgeVision.add("visionLearningArea", dbLabel , "databaseName", dbName)
            knowledgeVision.add("visionLearningArea", dbLabel , "headPitch", str(headPitch))
            visionReco.changeDatabase(dbPath,dbName)
            #visionReco.clearCurrentDatabase()

            size = visionReco.getSize()
            #size = 1
            self.logger.info("VisionReco database:::"+dbName + "label:::" +dbLabel +"size:::"+ str(size))

            for detailNode in databaseNode.iter("dataDetail"):
                imageFile = detailNode.find("imageFile").text
                imageName = imageFile
                imageLabelText = detailNode.find("imageLabel").text.encode("utf-8")
                imageLabel = imageLabelText.split(",")

                check = visionReco.learnFromFile(os.path.join(path ,imageFile), imageName, imageLabel, False, True)
                self.logger.info("Try VisionReco Record:::" + imageFile + "**result :::" + str(check))

                if check:
                    fileGroupRec.append(imageFile)
                else:
                    fileGroupNot.append(imageFile)
                    exeLog = exeLog + "画像ファイル" + imageFile + "が登録できていません。"

        self.memory.insertData("executedFileVisonLearning",fileGroupRec)
        self.memory.insertData("notRecPictureVisonLearning",fileGroupNot)
        self.memory.insertData("recentResultVisonLearning",exeLog)

        self.onStopped()

    def onInput_onStop(self):
        self.onUnload()]]>
