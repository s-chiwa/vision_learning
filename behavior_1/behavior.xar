                                    <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        pass

    def onInput_onStart(self):
        from naoqi import ALProxy
        import math

        names = list()
        times = list()
        keys = list()
        memory= ALProxy("ALMemory")
        motion = ALProxy("ALMotion")

        pitch = self.memory.getData("poseRadianVisionLearning")

        anglesPitch = motion.getAngles("HeadPitch", False)
        degreePitch = pitch - anglesPitch[0]

        anglesYaw = motion.getAngles("HeadYaw", False)
        degreeYaw = 0 - anglesYaw[0]

        tiPitch =  math.fabs(3*degreePitch)
        tiYaw = math.fabs(3*degreeYaw)

        ti = 0
        if tiPitch >= tiYaw:
            ti = tiPitch
        else:
            ti = tiYaw

        names.append("HeadPitch")
        times.append(ti)
        keys.append([pitch])

        names.append("HeadYaw")
        times.append(ti)
        keys.append([0])

        try:
            motion = ALProxy("ALMotion")
            motion.angleInterpolation(names, keys, times, True)
            self.onStopped()
        except:
            self.logger.warning("error!")

        pass

    def onInput_onStop(self):
        self.onUnload()]]>
