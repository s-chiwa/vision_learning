                                    <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        pass

    def onLoad(self):
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        self.memory = None

    def onInput_onStart(self, p):
        import math
        walk = self.memory.getData("walkDistMoveTo")
        walkLittle = self.memory.getData("walkLittleDistMoveTo")
        if p == "goForward":
            d = walk
            self.memory.insertData("distanceMoveTo",d)
            self.memory.insertData("timeMoveTo",d)
            self.memory.insertData("turnMoveTo",0.0)
            self.onStopped()

        if p == "goForwardLittle":
            d = walkLittle
            self.memory.insertData("distanceMoveTo",d)
            self.memory.insertData("timeMoveTo",d)
            self.memory.insertData("turnMoveTo",0.0)
            self.onStopped()

        if p == "goBack":
            d = -1*walk
            self.memory.insertData("distanceMoveTo",d)
            self.memory.insertData("timeMoveTo",math.fabs(d))
            self.memory.insertData("turnMoveTo",0.0)
            self.onStopped()

        if p == "goBackLittle":
            d = -1*walkLittle
            self.memory.insertData("distanceMoveTo",d)
            self.memory.insertData("timeMoveTo",math.fabs(d))
            self.memory.insertData("turnMoveTo",0.0)
            self.onStopped()

        if p == "goRight":
            t = -90
            self.memory.insertData("distanceMoveTo",0.0)
            self.memory.insertData("timeMoveTo",1.0)
            self.memory.insertData("turnMoveTo",t)
            self.onStopped()

        if p == "goLeft":
            t = 90
            self.memory.insertData("distanceMoveTo",0.0)
            self.memory.insertData("timeMoveTo",1.0)
            self.memory.insertData("turnMoveTo",t)
            self.onStopped()

        if p == "goRightForward":
            t = -45
            self.memory.insertData("distanceMoveTo",0.0)
            self.memory.insertData("timeMoveTo",1.0)
            self.memory.insertData("turnMoveTo",t)
            self.onStopped()

        if p == "goLeftForward":
            t = 45
            self.memory.insertData("distanceMoveTo",0.0)
            self.memory.insertData("timeMoveTo",1.0)
            self.memory.insertData("turnMoveTo",t)
            self.onStopped()

        if p == "goRightBack":
            t = -135
            self.memory.insertData("distanceMoveTo",0.0)
            self.memory.insertData("timeMoveTo",1.0)
            self.memory.insertData("turnMoveTo",t)
            self.onStopped()

        if p == "goLeftBack":
            t = 135
            self.memory.insertData("distanceMoveTo",0.0)
            self.memory.insertData("timeMoveTo",1.0)
            self.memory.insertData("turnMoveTo",t)
            self.onStopped()

        if p == "goTurnBack":
            t = 180
            self.memory.insertData("distanceMoveTo",0.0)
            self.memory.insertData("timeMoveTo",1.0)
            self.memory.insertData("turnMoveTo",t)
            self.onStopped()

        pass
    def onInput_onStop(self):
        self.onUnload()]]>
